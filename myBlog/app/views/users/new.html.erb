<%= render 'shared/title', title: 'Sign up'%>

<%= form_for(@user) do |f| %>
  <p><%= f.text_field :username, :placeholder => "username" %></p>
  <p><%= f.text_field :nickname, :placeholder => "nickname" %></p>
  <p><%= f.password_field :password, :placeholder => "password" %></p>
  <%= f.submit class: "btn btn-primary" %>

<% end %>